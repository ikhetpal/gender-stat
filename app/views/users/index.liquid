<h1>Users</h1>
<h2> <input type="text" id="search-users-input" name="search" value="" placeholder="Name"> <button type="button" class="search-users-submit">Submit</button> <button type="button" class="back-button">Back</button> <button type="button" class="home-button">Home</button></h2>

{% for user in users %}
  <p>Name: {{ user.full_name }}</p>
  <p>Age: {{ user.age }}</p>
  <p>Gender: {{ user.gender }}</p>
  <p>Created At: {{ user.created_at }}</p>
  <hr>
{% endfor %}
